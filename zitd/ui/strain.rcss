{

div#logo
{
	width: 700px;
	height: 150px;
	padding-top: 50px;
	float:left;
}

div#logo_image
{
	width: 328px;
	height: 101px;
	logo-decorator: image;
    logo-image: strain_logo.png;
    margin-left: auto;
	margin-right: auto;
	float: left;
}

div.right_button
{
	width: 165px;
	height: 40px;
	margin-right: 5px;
	background: #8b0000;
	float:right;
}


div.button_text
{
	width: 165px;
	height: 20px;
	margin: 8px 0px 12px 0px;
	float: left;
	color: white;
	font-size: 14px;
	text-align: center;
	shadow-font-effect: shadow;
	shadow-offset: 1px 1px;
	shadow-color: black;
}

div.middle_button:hover,
div.right_button:hover,
div.middle_button_default:hover
{
	background: #ab2020;
}

div.middle_button:active,
div.right_button:active,
div.middle_button_default:active,
{
	background: #8b0000;
}

div.item_image
{
	width: 32px;
	height: 32px;
	margin: 9px;
	float: left;
}

div.item_text
{
	shadow-font-effect: shadow;
	shadow-offset: 1px 1px;
	shadow-color: black;
}

div.item_name
{
	padding-top: 5px;
}

